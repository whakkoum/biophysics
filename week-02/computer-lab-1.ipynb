# Assignment 2
def w(t,A,tau):
    return A*(np.e**(-t/tau)-1+t/tau)

t = np.linspace(0,2,1000)
plt.figure()
plt.plot(t,w(t,1,1), color = 'green', linestyle = 'dotted')
plt.plot(t,w(t,2,2), color = 'red', linestyle = 'solid')
plt.plot(t,w(t,3,3), color = 'blue', linestyle = 'dashed')
plt.xlabel('time')
plt.ylabel('W(t)')
plt.legend(('W1','W2','W3'))
plt.show()


